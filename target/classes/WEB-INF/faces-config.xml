<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
    
    <application>
      <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>	
    
  
    <managed-bean>
	   <managed-bean-name>helloWorld</managed-bean-name>
	   <managed-bean-class>Pruebas_Bean.HelloWorld</managed-bean-class>
	   <managed-bean-scope>request</managed-bean-scope>
	</managed-bean> 
	
	<managed-bean>
	   <managed-bean-name>message</managed-bean-name>
	   <managed-bean-class>Pruebas_Bean.Message</managed-bean-class>
	   <managed-bean-scope>request</managed-bean-scope>
	</managed-bean> 
    
		<managed-bean>
	   <managed-bean-name>BancoMB</managed-bean-name>
	   <managed-bean-class>com.edu.uniajc.cajero.bean.BancoManagedBean</managed-bean-class>
	   <managed-bean-scope>request</managed-bean-scope>
	</managed-bean> 
    
</faces-config>
